rim () {
  rmate index.js
}

www.rb () {
  jump routes && vs www.rb && rmate www.rb
}

api.rb () {
  jump routes && vs api.rb && rmate api.rb
}

lint () {
 npm run lint-this $1
}

sds () {
  cpr derbent
  ssh delivery-staging
  cpr v
}

goproduction () {
	cpr sell-staging
  ssh production-rentify-worker
	cpr v
}

rstart () {
  cpr reactsryroutes
  cd ~/rentify/rentify
  ./bin/webpack-dev-server
}


indexit () {
  mv *js index.js
}

routes () {
  jump rroutes && vs . && rmate referencesRoutes.js
}

troutes () {
  jump troutes && vs . && rmate tenantMakeOfferRoutes.js
}

rfy () {
  vs ~/zfiles/rfy && rmate ~/zfiles/rfy
}

consoleHistory () {
  echo "puts Readline::HISTORY.entries.each { |c| puts c }"
}

migrate () {
  jump main && ./bin/rake db:migrate
}

models () {  
  jump models && vs . && rmate reference.rb && ls reference
}

reducers () {  
  jump reducers && vs . && rmate referencesReducers.js
}

makeoffer () {  
  jump makeoffer && vs index.js && rmate index.js 
}

form () {
  jump form && rmate index.js
}

offer () {  
  jump offer && vs index.js && rmate index.js 
}

landstart () {
  jump landstart && rim && vs .
}

components () {
  jump components && vs . && rmate .
}

actions () {
  jump actions && vs . && rmate referencesActions.js
}
api () {
  jump actions && vs . && rmate api.js
}

trefs () {
  jump trefs && rmate tenant_referees_controller.rb
}

refs () {
  jump refs && rmate references_controller.rb
}

staging () {
  cpr sell-staging
  ssh sell-staging
  cpr v
}
