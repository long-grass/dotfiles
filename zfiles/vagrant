alias coro='cd /Volumes/vagrant/vagrant/coreos-vagrant && vagrant up && vagrant ssh'
alias showv='cd ~/vagrant/vagrant_home/boxes && du -sh * && cd -'
alias addvuser='usermod -a -G docker vagrant'
alias vup='vagrant up'
alias vgs="vagrant global-status"

vrun () {
  eval 'vagrant ssh -c'" \"${@:1} \""
}

vkill () {
  vagrant global-status | grep running | cut -c 1-9 | while read line; do echo $line; vagrant suspend $line; done
  cd 
}

govm () {
  change-profile stretch
  vagrant ssh
  change-profile v
}

vk (){
  change-profile kemerovo
  cd /Volumes/vagrant/kemerovo
  vagrant ssh
  change-profile v
}


startvk() { # vagrant global-status | grep kemesrovo | awk '{print $1}' 
  cd /Volumes/vagrant/kemerovo &&  vagrant up
}

